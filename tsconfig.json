{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ES2022",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"]
    },
    "types": ["element-plus/global"]
  },
  "exclude": ["node_modules", "dist"]
}
